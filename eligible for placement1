import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        String name = sc.nextLine();       // e.g., "John"
        int arrears = sc.nextInt();        // e.g., 1
        double cgpa = sc.nextDouble();     // e.g., 76.5
        
        boolean eligible = false;

        if ((arrears == 1 && cgpa > 70) || (arrears == 2 && cgpa > 75)) {
            eligible = true;
        }

        if (eligible) {
            System.out.println("Name of the Student:"+name);
            System.out.println("John is Eligible for Placement");
        } else {
            System.out.println("Name of the Student:"+name);
            System.out.println("John is Not Eligible for Placement");
        }
    }
}

